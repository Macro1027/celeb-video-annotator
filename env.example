# Celebrity Video Annotator Environment Variables
# Copy this file to .env and set your actual values

# Pinecone API Key (Required for face recognition database)
PINECONE_API_KEY=your_pinecone_api_key_here

# Server Configuration
HOST=0.0.0.0
PORT=8000

# Application Settings
OUTPUT_DIR=results/
DEBUG_CONFIG=0

# Optional: Target specific celebrity for highlighting
TARGET_LABEL=

# Redis Configuration (usually defaults are fine)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0 